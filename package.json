{
	"name": "electron-express-react-demo",
	"version": "1.0.0",
	"description": "A minimal Electron application",
	"main": "main.js",
	"scripts": {
		"start": "chcp 65001 && electron .",
		"dev": "concurrently \"webpack --watch\" \"electron .\"",
		"win": "electron-packager . --electronVersion=26.0.0 --overwrite --platform=win32 --arch=x64 --out=out --icon=assets/app-icon/win/app.ico --ignore=\"(^/config|^/src|^/out|^/routers|^/gulp|.vscode|.github|.gitignore|.prettierrc.js|.babelrc|^/webpack|exemple)\" && gulp --gulpfile gulp/gulpfile.js setEnvProduction",
		"clearOut": "gulp --gulpfile gulp/gulpfile.js clearOut",
		"kill": "gulp --gulpfile gulp/gulpfile.js killProcess"
	},
	"repository": "https://github.com/electron/electron-quick-start",
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo"
	],
